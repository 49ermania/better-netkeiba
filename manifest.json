{
  "manifest_version": 3,
  "name": "Better netkeiba",
  "version": "1.1",
  "icons": {
    "128": "128.png"
  },
  "description": "This extension for better netkeiba.",
  "content_scripts": [{
    "matches": [
      "https://db.netkeiba.com/horse/*",
      "https://*.sp.netkeiba.com/*"
    ],
    "js": [
      "content-script.js"
    ],
    "run_at": "document_start"
  }],
  "background": {
    "service_worker": "background.js"
  }
}
